<?xml version='1.0' encoding='utf-8'?>
<widget id="io.github.dineshcondur.primer" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>typeCast</name>
    <description>
        Android keyboard test.
    </description>
    <author email="dinesh.condur@gmail.com" href="">
        Dinesh Condur
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="android" spec="~7.0.0" />
    <plugin name="cordova-plugin-geolocation" spec="3.0.0" />
    <plugin name="cordova-plugin-dialogs" spec="1.3.4" />
    <plugin name="cordova-plugin-statusbar" spec="2.4.0" />
</widget>
